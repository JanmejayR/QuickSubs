{
    "build": [
      {
        "service": "backend",
        "command": "cd backend && npm install && npm run build"
      },
      {
        "service": "frontend",
        "command": "cd frontend && npm install && npm run build"
      }
    ],
    "start": {
      "backend": "cd backend && npm start",
      "frontend": "cd frontend && npm run start"
    }
  }
  